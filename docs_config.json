{
    "tags": {
      "allowUnknownTags": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "logoFile": "img/logo.png",
      "cleverLinks": true,
      "monospaceLinks": false,
      "dateFormat": "ddd MMM Do YYYY",
      "outputSourceFiles": true,
      "outputSourcePath": true,
      "systemName": "Mediumroast for GitHub SDK and CLI",
      "footer": "",
      "copyright": "Â© 2023-2024 Mediumroast, Inc. All rights reserved.",
      "navType": "inline",
      "theme": "cyborg",
      "linenums": true,
      "collapseSymbols": false,
      "inverseNav": true,
      "protocol": "html://",
      "methodHeadingReturns": false
    },
    "markdown": {
      "parser": "gfm",
      "hardwrap": true
    },
    "opts": {
      "template": "node_modules/ink-docstrap/template",
      "destination": "./docs/"
    }
  }
  